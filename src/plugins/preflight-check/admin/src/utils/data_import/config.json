{
	"participant": {
        "pk": "id",
        "type":"attributes as heading",
		"sheet": "Basic Data",
		"api": "api::nwc-participant.nwc-participant",
		"lookup": {
			"ID": "id",
			"Last Name": "last_name",
			"First Name": "first_name",
			"Middle Name and/or Initial 1": "middle_name_initial",
			"Middle Name and/or Initial 2": "middle_name_initial_2",
			"Nickname": "nickname",
			"State": "represented_state",
